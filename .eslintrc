{
  "extends": ["oclif", "oclif-typescript"],
  "rules": {
    "arrow-parens": ["error", "always"],
    "comma-dangle": ["error", "never"],
    "indent": ["error", 2, { "MemberExpression": 1 }],
    "object-curly-spacing": ["error", "always"],
    "operator-linebreak": [
      "error",
      "after",
      { "overrides": { "?": "before", ":": "before" } }
    ],
    "semi": ["error", "always"]
  }
}
