{
  "name": "@cdwr/cli",
  "description": "Codeware Sthlm Developer CLI",
  "version": "0.0.0-development",
  "author": "Codeware Sthlm",
  "bin": {
    "cdwr": "./bin/run"
  },
  "bugs": "https://github.com/codeware-sthlm/cdwr-cli/issues",
  "dependencies": {
    "@oclif/core": "^2",
    "@oclif/plugin-autocomplete": "^1.4.3",
    "@oclif/plugin-commands": "^2.2.5",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-not-found": "^2.3.17",
    "@oclif/plugin-search": "^0.0.11",
    "@oclif/plugin-update": "^3.1.2",
    "@oclif/plugin-version": "^1.2.1",
    "@oclif/plugin-warn-if-update-available": "^2.0.24",
    "inquirer": "^8.2.3",
    "verdaccio": "^5.20.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@oclif/test": "^2.3.3",
    "@types/chai": "^4",
    "@types/inquirer": "^8.2.3",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.18.11",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.3",
    "husky": "^8.0.3",
    "mocha": "^9",
    "oclif": "^3",
    "semantic-release": "^20.1.0",
    "shx": "^0.3.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "typescript": "^4.9.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/codeware-sthlm/cdwr-cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "oclif": {
    "bin": "cdwr",
    "dirname": "cdwr",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-autocomplete",
      "@oclif/plugin-commands",
      "@oclif/plugin-help",
      "@oclif/plugin-not-found",
      "@oclif/plugin-search",
      "@oclif/plugin-update",
      "@oclif/plugin-version",
      "@oclif/plugin-warn-if-update-available"
    ],
    "topicSeparator": " ",
    "topics": {
      "registry": {
        "description": "Manage registry location"
      }
    }
  },
  "private": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": "codeware-sthlm/cdwr-cli",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "release:local": "oclif pack tarballs",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md",
    "semantic-release": "semantic-release"
  },
  "types": "dist/index.d.ts"
}
